@{
    ViewData["Title"] = "Profile";
    var account = ViewBag.account;
}
<h2>Your Profile </h2>
<div class="row">
    <div class="col-md-4">
        <img src="@account.Avatar" alt=" " style="width:100px"/>
    </div>
    <div class="col-md-8">
        <h2>Full Name:@account.Name</h2>
        <h3>Email:@account.Email</h3>
        <h3>Phone:@account.Phone</h3>
        <h3>Address;</h3>
    </div>
</div>
